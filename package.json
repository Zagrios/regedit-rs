{
  "name": "regedit-rs",
  "version": "1.0.3",
  "main": "index.js",
  "types": "index.d.ts",
  "keywords": [
    "regedit-rs",
    "regedit",
    "registry",
    "windows",
    "rust",
    "node regedit",
    "node registry"
  ],
  "files": [
    "index.d.ts",
    "index.js",
    "js-binding.js",
    "js-binding.d.ts"
  ],
  "napi": {
    "binaryName": "regedit-rs",
    "targets": [
      "aarch64-pc-windows-msvc",
      "x86_64-pc-windows-msvc",
      "i686-pc-windows-msvc"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "3.4.1",
    "@swc-node/register": "1.11.1",
    "@swc/core": "1.15.2",
    "@types/node": "24.10.1",
    "@types/winreg": "1.2.36",
    "ava": "6.4.1",
    "benny": "^3.7.1",
    "esbuild": "^0.27.0",
    "regedit": "5.1.4",
    "typescript": "5.4.2",
    "winreg": "1.2.5"
  },
  "ava": {
    "require": [
      "@swc-node/register"
    ],
    "extensions": [
      "ts"
    ],
    "timeout": "3m",
    "concurrency": 1,
    "environmentVariables": {
      "TS_NODE_PROJECT": "./tsconfig.json"
    }
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release --js js-binding.js --dts js-binding.d.ts",
    "build:debug": "napi build --platform --js js-binding.js --dts js-binding.d.ts",
    "prepublishOnly": "napi prepublish -t npm && esbuild js-binding.js --minify --allow-overwrite --outfile=js-binding.js",
    "test": "ava --verbose __test__/regedit-rs.*.*",
    "bench": "node -r @swc-node/register benchmark/bench.ts",
    "universal": "napi universal",
    "version": "napi version"
  },
  "packageManager": "yarn@4.0.2",
  "repository": "git@github.com:Zagrios/regedit-rs.git",
  "description": "A high-performance Windows registry toolkit, powered by Rust with napi-rs",
  "volta": {
    "node": "24.11.1",
    "yarn": "4.7.0"
  }
}
