{
    "name": "regedit-rs",
    "version": "0.0.0",
    "main": "index.js",
    "types": "index.d.ts",
    "keywords": [
        "regedit-rs",
        "regedit",
        "registry",
        "windows",
        "rust",
        "node regedit"
    ],
    "files": [
        "index.d.ts",
        "index.js",
        "js-binding.js",
        "js-binding.d.ts"
    ],
    "napi": {
        "name": "regedit-rs",
        "triples": {
            "defaults": false,
            "additional": [
                "aarch64-pc-windows-msvc",
                "x86_64-pc-windows-msvc",
                "i686-pc-windows-msvc"
            ]
        }
    },
    "license": "MIT",
    "devDependencies": {
        "@napi-rs/cli": "^2.16.5",
        "@swc-node/register": "^1.6.8",
        "@swc/core": "^1.3.99",
        "@types/node": "^20.10.0",
        "ava": "^5.3.1",
        "esbuild": "^0.19.8",
        "typescript": "^5.3.2"
    },
    "ava": {
        "require": [
            "@swc-node/register"
        ],
        "extensions": [
            "ts"
        ],
        "timeout": "3m",
        "environmentVariables": {
            "TS_NODE_PROJECT": "./tsconfig.json"
        }
    },
    "engines": {
        "node": ">= 10"
    },
    "scripts": {
        "artifacts": "napi artifacts",
        "build": "napi build --platform --release --js js-binding.js --dts js-binding.d.ts",
        "build:debug": "napi build --platform --js js-binding.js --dts js-binding.d.ts",
        "prepublishOnly": "napi prepublish -t npm && esbuild js-binding.js --minify --allow-overwrite --outfile=js-binding.js",
        "test": "ava __test__/regedit-rs.*.*",
        "bench": "ava __test__/benchmarks.*.*",
        "universal": "napi universal",
        "version": "napi version"
    },
    "packageManager": "yarn@4.0.2"
}
